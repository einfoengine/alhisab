{
  "name": "alhisab-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build": "echo \"Building the backend...\"",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "test:unit": "echo \"Running unit tests...\"",
    "test:integration": "echo \"Running integration tests...\"",
    "test:e2e": "echo \"Running end-to-end tests...\"",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:debug:watch": "node --inspect-brk node_modules/.bin/jest --watch",
    "test:debug:coverage": "node --inspect-brk node_modules/.bin/jest --coverage",
    "test:debug:watch:coverage": "node --inspect-brk node_modules/.bin/jest --watch --coverage",
    "test:debug:watch:coverage:ci": "node --inspect-brk node_modules/.bin/jest --watch --coverage --ci",
    "test:debug:watch:coverage:ci:report": "node --inspect-brk node_modules/.bin/jest --watch --coverage --ci --reporters=default jest-junit",
    "test:debug:watch:coverage:ci:report:html": "node --inspect-brk node_modules/.bin/jest --watch --coverage --ci --reporters=default jest-junit --reporters=jest-html-reporter",
    "test:debug:watch:coverage:ci:report:html:json": "node --inspect-brk node_modules/.bin/jest --watch --coverage --ci --reporters=default jest-junit --reporters=jest-html-reporter --reporters=jest-json-reporter",
    "test:debug:watch:coverage:ci:report:html:json:lcov": "node --inspect-brk node_modules/.bin/jest --watch --coverage --ci --reporters=default jest-junit --reporters=jest-html-reporter --reporters=jest-json-reporter --reporters=jest-lcov-reporter",
    "test:debug:watch:coverage:ci:report:html:json:lcov:teamcity": "node --inspect-brk node_modules/.bin/jest --watch --coverage --ci --reporters=default jest-junit --reporters=jest-html-reporter --reporters=jest-json-reporter --reporters=jest-lcov-reporter --reporters=jest-teamcity-reporter",
    "test:debug:watch:coverage:ci:report:html:json:lcov:teamcity:junit": "node --inspect-brk node_modules/.bin/jest --watch --coverage --ci --reporters=default jest-junit --reporters=jest-html-reporter --reporters=jest-json-reporter --reporters=jest-lcov-reporter --reporters=jest-teamcity-reporter --reporters=jest-junit-reporter",
    "test:debug:watch:coverage:ci:report:html:json:lcov:teamcity:junit:json": "node --inspect-brk node_modules/.bin/jest --watch --coverage --ci --reporters=default jest-junit --reporters=jest-html-reporter --reporters=jest-json-reporter --reporters=jest-lcov-reporter --reporters=jest-teamcity-reporter --reporters=jest-junit-reporter --reporters=jest-json-reporter",
    "test:debug:watch:coverage:ci:report:html:json:lcov:teamcity:junit:json:lcov": "node --inspect-brk node_modules/.bin/jest --watch --coverage --ci --reporters=default jest-junit --reporters=jest-html-reporter --reporters=jest-json-reporter --reporters=jest-lcov-reporter --reporters=jest-teamcity-reporter --reporters=jest-junit-reporter --reporters=jest-json-reporter --reporters=jest-lcov-reporter"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "dotnav": "^0.0.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "mongoose": "^8.14.1",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.10",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
